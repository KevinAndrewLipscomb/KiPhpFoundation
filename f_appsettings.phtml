function AppSettings($path_to_app_root = "")
   {
   $saved_error_level = error_reporting(E_ALL);
   $ini = parse_ini_file($path_to_app_root . 'unpublished.ini',TRUE) + parse_ini_file($path_to_app_root . 'app.ini',TRUE);
   error_reporting($saved_error_level);
   return $ini;
   }
?>
